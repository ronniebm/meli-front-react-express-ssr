{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Mercadolibre Colombia",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf build",
    "build:server": "webpack --config webpack.server.config.js",
    "start:server": "node dist/server/bundle.js",
    "build:client": "webpack --config webpack.client.config.js",
    "build": "run-p dev:client dev:server",
    "dev:server": "nodemon --ignore build --exec 'run-s clear build:server start:server'",
    "dev:client": "webpack serve --config webpack.client.config.js",
    "dev:watch": "webpack serve --open",
    "dev": "run-p dev:client dev:server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/icons-material": "5.8.4",
    "axios": "0.27.2",
    "express": "4.18.1",
    "nodemon": "2.0.19",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "rimraf": "3.0.2",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-react": "7.18.6",
    "@types/node-sass": "4.11.3",
    "@types/react-dom": "18.0.6",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "2.6.1",
    "sass": "1.54.0",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  }
}
